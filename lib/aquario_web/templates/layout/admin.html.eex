        <img class="logo" src="https://aquario-staging.s3.us-east-2.amazonaws.com/manual/logo-svg.svg" alt="Logo do Aqua Rio">
      </div>
    <% end %>

    <!-- Divider -->
    <hr class="sidebar-divider my-0">

    <!-- Nav Item - Dashboard -->
    <li class="nav-item active">      
      <%= link to: admin_page_path(@conn, :index), class: "nav-link" do %>
        <i class="fas fa-fw fa-tachometer-alt text-white-50"></i>
        <span>Dashboard</span>
      <% end %>
    </li>

    <!-- Divider -->
    <hr class="sidebar-divider">

    <!-- Heading -->
    <div class="sidebar-heading">
      Interface
    </div>

    <!-- Nav Item - Pages Collapse Menu -->
    <li class="nav-item">
      <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true"
        aria-controls="collapseTwo">
        <i class="fas fa-fw fa-align-justify"></i>
        <span>Tanques</span>
      </a>
      <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
        <div class="bg-white py-2 collapse-inner rounded">
          <h6 class="collapse-header">Administrar Tanques</h6>
          <a><%= link "Listar Tanques", to: admin_tank_path(@conn, :index), class: "collapse-item"%></a>
          <a><%= link "Novo Tanque", to: admin_tank_path(@conn, :new), class: "collapse-item"%></a>
        </div>
      </div>
    </li>

    <!-- Nav Item - Researches Collapse Menu -->
    <li class="nav-item">
      <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapsespecy" aria-expanded="true"
        aria-controls="collapsespecy">
        <i class="fas fa-fw fa-fish"></i>
        <span>Espécies</span>
      </a>
      <div id="collapsespecy" class="collapse" aria-labelledby="headingspecy" data-parent="#accordionSidebar">
        <div class="bg-white py-2 collapse-inner rounded">
          <h6 class="collapse-header">Administrar Espécies:</h6>
          <a><%= link "Listar Espécies", to: admin_specy_path(@conn, :index), class: "collapse-item" %></a>
          <a><%= link "Nova Espécie", to: admin_specy_path(@conn, :new), class: "collapse-item" %></a>
        </div>
      </div>
    </li>

    <!-- Nav Item - Researches Collapse Menu -->
    <li class="nav-item">
      <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseUtilities" aria-expanded="true"
        aria-controls="collapseUtilities">
        <i class="fas fa-fw fa-search"></i>
        <span>Pesquisas</span>
      </a>
      <div id="collapseUtilities" class="collapse" aria-labelledby="headingUtilities" data-parent="#accordionSidebar">
        <div class="bg-white py-2 collapse-inner rounded">
          <h6 class="collapse-header">Administrar Pesquisas:</h6>
          <a><%= link "Listar Pesquisas", to: admin_research_path(@conn, :index), class: "collapse-item" %></a>
          <a><%= link "Nova Pesquisa", to: admin_research_path(@conn, :new), class: "collapse-item" %></a>
        </div>
      </div>
    </li>

    <!-- Divider -->
    <hr class="sidebar-divider d-none d-md-block">

    <!-- Sidebar Toggler (Sidebar) -->
    <div class="text-center d-none d-md-inline">
      <button class="rounded-circle border-0" id="sidebarToggle"></button>
    </div>

